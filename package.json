{
	"name": "MY_APP",
	"version": "1.0.0",
	"ts-project-version": "1.2.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"start": "npm run dev",
		"dev": "npm run lint && ts-node src/index.ts",
		"test": "npm run lint && npm run webpack:dev && npm run build:spec && npm run coverage",
		"build": "npm run build:dev",
		"build:dev": "npm run lint && npm run webpack:dev",
		"build:spec": "tsc --p tsconfig.spec.json",
		"build:prod": "npm run preprocess prod && npm run lint:prod && npm run webpack:prod && npm run postprocess prod",
		"webpack:dev": "./node_modules/.bin/webpack --mode=development",
		"webpack:prod": "./node_modules/.bin/webpack --mode=production",
		"preprocess": "node preprocess.js",
		"postprocess": "node postprocess.js",
		"doc": "typedoc --out docs src --excludeNotExported",
		"lint": "eslint . --ext .ts --fix",
		"lint:prod": "eslint . --ext .ts --fix --config=.eslintrc.prod.json",
		"coverage": "nyc npm run coverage-test",
		"coverage-test": "jasmine --config=jasmine.json",
		"coveralls": "cat ./coverage/lcov.info | coveralls"
	},
	"directories": {
		"test": "test"
	},
	"files": [
		"dist/*"
	],
	"dependencies": {
		"dotenv": "^8.2.0",
		"source-map-support": "^0.5.19"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.1",
		"@types/jasmine": "^3.6.2",
		"@types/node": "^12.19.13",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"coveralls": "^3.1.0",
		"eslint": "^7.17.0",
		"jasmine": "^3.6.3",
		"nyc": "^15.1.0",
		"ts-loader": "^8.0.12",
		"ts-node": "^9.1.1",
		"typedoc": "^0.19.2",
		"typescript": "^4.1.3",
		"webpack": "^5.10.1",
		"webpack-cli": "^4.2.0"
	}
}
