{
	"name": "MY_APP",
	"version": "0.0.0",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"scripts": {
		"test": "npm run build:spec && jasmine --config=jasmine",
		"build": "tsc --p tsconfig.json",
		"build:spec": "tsc --p tsconfig.spec.json",
		"dev": "ts-node src/main.ts",
		"prepare": "npm run build",
		"doc": "typedoc --out docs src --excludeNotExported"
	},
	"directories": {
		"test": "test"
	},
	"files": [
		"dist/*"
	],
	"dependencies": {
		"dotenv": "^8.2.0"
	},
	"devDependencies": {
		"@types/node": "^12.12.58",
		"@types/jasmine": "^3.5.14",
		"jasmine": "^3.6.1",
		"ts-node": "^9.0.0",
		"tslint": "^6.1.3",
		"tslint-eslint-rules": "^5.4.0",
		"typedoc": "^0.19.2",
		"typescript": "^4.0.2"
	}
}
